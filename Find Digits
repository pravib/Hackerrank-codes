#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int t;cin>>t;
  while(t--)
    {
    long long n;cin>>n;
    long long temp=n,count=0;
    while(temp)
      {
      long long dig=temp%10;
      if(dig&&n%dig==0) count++;
      temp=temp/10;
      
    }
    cout<<count<<endl;
  }
    return 0;
}
